{
  "name": "backend-exam-v2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "prestart": "ts-node scripts/populate-db.ts",
    "start": "yarn run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "yarn prestart && cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@tsed/ajv": "^7.20.0",
    "@tsed/cli-core": "4.4.6",
    "@tsed/common": "^7.20.0",
    "@tsed/core": "^7.20.0",
    "@tsed/di": "^7.20.0",
    "@tsed/engines": "^7.20.0",
    "@tsed/exceptions": "^7.20.0",
    "@tsed/json-mapper": "^7.20.0",
    "@tsed/logger": "^6.3.4",
    "@tsed/logger-file": "^6.3.4",
    "@tsed/mongoose": "^7.20.0",
    "@tsed/openspec": "^7.20.0",
    "@tsed/platform-cache": "^7.20.0",
    "@tsed/platform-exceptions": "^7.20.0",
    "@tsed/platform-express": "^7.20.0",
    "@tsed/platform-log-middleware": "^7.20.0",
    "@tsed/platform-middlewares": "^7.20.0",
    "@tsed/platform-params": "^7.20.0",
    "@tsed/platform-response-filter": "^7.20.0",
    "@tsed/platform-views": "^7.20.0",
    "@tsed/schema": "^7.20.0",
    "@tsed/swagger": "^7.20.0",
    "@typegoose/typegoose": "^11.0.0",
    "ajv": "^8.12.0",
    "barrelsby": "^2.5.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "method-override": "^3.0.0",
    "mongoose": "^7.0.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.4.6",
    "@tsed/cli-plugin-jest": "4.4.6",
    "@tsed/cli-plugin-mongoose": "4.4.6",
    "@tsed/testing-mongoose": "^7.20.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^8.2.4",
    "@types/jest": "^29.5.0",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default",
    "architecture": "default"
  }
}
